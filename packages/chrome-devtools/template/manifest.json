{
  "manifest_version": 2,
  "name": "pbkit-devtools",
  "version": "0.0.6",
  "description": "pbkit devtools",
  "icons": {
    "128": "icons/pbkit-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "devtools_page": "devtools.html"
}
